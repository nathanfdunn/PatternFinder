
(setEcho false)

(readTable "seasonal.csv")
(set $table $_)

(tokenize $table 20)

(tokenize $table 0.5)

(set $stream $_)

(extract $stream 100)

(set $patterns $_)

(print $_)

(getInd $patterns 4)

(set $p $_)
(print $p)

(getMatches $p)
(set $matchList $_)
(print $matchList)
(getInd $matchList 2)
(print $_)

(readTable "nfdunn_GISP2_formatted.csv")
(set $table $_)
(subTable $table "Ca" "Cl" "K" "Mg" "Na" "NH4" "NO3" "SO4" "B2K")
(set $table $_)

(tokenize $table 100)
(set $stream $_)
(print $stream)

(parsePattern "Ca^~Kv:[0,0]")
(set $pattern $_)
(completeTime $stream $pattern)

(print $_)


(parsePattern "Ca^~Na-:[0,0]")
(set $pattern $_)
(completeTime $stream $pattern)

(set $pattern $_)

(match $stream $pattern)
(set $evalObj $_)
(getMatches $evalObj)
(set $matches $_)
(getInd $matches 0)
(set $m $_)
(display $stream $m)
(displayStream $stream)
(displayStream $stream $m)




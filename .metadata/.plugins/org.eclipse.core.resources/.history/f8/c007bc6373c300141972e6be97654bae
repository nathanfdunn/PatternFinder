	private double[][] createPartition(double times, double chunkWidth){
		
	}
	
	//Named after the R function
	private double[] seq(double start, double end, double chunkWidth){
		final double eps = 0.0001;
		int numChunks = (int)Math.round((end-start)/chunkWidth);
		double[] out = new double[numChunks+1];
		out[0] = start - eps;
		for (int i=0; i<numChunks; i++){
			
		}
		out[numChunks] = end + eps;
	}
	
	private double[] seq(double start, double end, int numChunks){
		double chunkWidth = (end-start)/numChunks;
		return seq(start, end, chunkWidth);
	}

}

package ui;

import java.util.HashMap;

public class CommandApp {

	private IInputReader input;
	private boolean quit;
	private HashMap<K, V>
	
	
	public void execute(String cmd){
		
		
	}
	
	public void run(){
		while (!quit){
			String cmd = input.getInput();
			execute(cmd);
